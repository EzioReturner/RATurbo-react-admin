{
  "presets": [
    "react-app"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    "@babel/plugin-syntax-jsx",
    "@babel/plugin-proposal-export-default-from",
    [
      "import", {
        "libraryName": "antd",
        "style": true
      }
    ]
  ]
}
